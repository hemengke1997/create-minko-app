{
  "name": "react-typescript-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo vite:dev",
    "build": "turbo vite:build",
    "vite:dev": "vite",
    "vite:build": "vite build --mode production",
    "build:test": "vite build --mode test",
    "lint-staged": "turbo turbo:lint-staged",
    "turbo:lint-staged": "lint-staged",
    "lint-staged:js": "turbo turbo:lint-staged:js",
    "turbo:lint-staged:js": "eslint --cache --fix",
    "preview": "vite preview",
    "report": "cross-env REPORT=true pnpm run build",
    "up": "pnpm update --i --L",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@minko-fe/axios-pro": "^0.1.19",
    "@minko-fe/lodash-pro": "^0.2.1",
    "@minko-fe/react-component": "^0.1.19",
    "@minko-fe/react-hook": "^0.2.1",
    "@minko-fe/react-route": "^0.1.19",
    "@minko-fe/vite-config": "^0.1.19",
    "antd": "^5.11.1",
    "context-state": "^2.1.0",
    "framer-motion": "^6.3.16",
    "modern-flexible": "^0.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.10.0",
    "un-detector": "^0.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@minko-fe/commitlint-config": "^2.0.7",
    "@minko-fe/eslint-config": "^2.1.2",
    "@minko-fe/postcss-config": "^0.1.19",
    "@minko-fe/tsconfig": "^2.0.7",
    "@types/node": "^18.14.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "husky": "^8.0.1",
    "lint-staged": "13.1.2",
    "tailwindcss": "3.2.7",
    "turbo": "^1.12.2",
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vite-plugin-public-typescript": "^2.2.5"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "tailwindcss"
      ]
    }
  },
  "lint-staged": {
    "!(dist/**)/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  }
}
